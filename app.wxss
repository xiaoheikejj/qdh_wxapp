/**app.wxss**/
@import "/utils/icon.wxss";

page {
  width: 100%;
  height: 100%;
}
.body {
  overflow-x: hidden;
} 

.clearfix::after, .clearfix::before {
  content: "";
  display: table;
}

.clearfix::after {
  clear: both;
  overflow: hidden;
}

image, video {
  vertical-align: bottom;  
}

::-webkit-scrollbar{
  width: 0;
  height: 0;
  color: transparent;
}
button::after {
  border: none
}
input {
  outline: none;
  border: none;
  list-style: none
}